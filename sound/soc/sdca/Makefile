obj-$(CONFIG_SND_SOC_SDCA_SMART_AMP)	+= snd-soc-sdca-smart-amp.o
obj-$(CONFIG_SND_SOC_SDCA_SMART_MIC)	+= snd-soc-sdca-smart-mic.o
obj-$(CONFIG_SND_SOC_SDCA_UAJ)	+= snd-soc-sdca-uaj.o
obj-$(CONFIG_SND_SOC_SDCA_HID)	+= snd-soc-sdca-hid.o
